{
  "name": "@ito-frontend/eslint-config",
  "type": "module",
  "version": "3.0.0",
  "description": "ESLint Config for OneAD ITO Team. Powered by @antfu/eslint-config",
  "author": "ito-frontend",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ito-frontend/eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "onead",
    "ito"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --fix",
    "lint:rules": "npx @eslint/config-inspector",
    "release": "pnpm build && bumpp"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "4.1.1",
    "eslint-plugin-check-file": "^3.0.0",
    "eslint-plugin-tailwindcss": "^3.18.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.26.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "bumpp": "^10.0.1",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "lint-staged": "^15.4.3",
    "rollup": "^4.34.1",
    "simple-git-hooks": "^2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
