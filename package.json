{
  "name": "@ito-frontend/eslint-config",
  "type": "module",
  "version": "1.0.12",
  "description": "ESLint Config for OneAD ITO Team. Powered by @antfu/eslint-config",
  "author": "ito-frontend",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ito-frontend/eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "onead",
    "ito"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --fix",
    "lint:rules": "npx @eslint/config-inspector",
    "release": "pnpm build && bumpp"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "3.4.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.4"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.13.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "bumpp": "^9.5.2",
    "eslint": "^9.10.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "lint-staged": "^15.2.10",
    "rollup": "^4.21.2",
    "simple-git-hooks": "^2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
