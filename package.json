{
  "name": "@ito-frontend/eslint-config",
  "type": "module",
  "version": "2.0.4",
  "description": "ESLint Config for OneAD ITO Team. Powered by @antfu/eslint-config",
  "author": "ito-frontend",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ito-frontend/eslint-config.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "onead",
    "ito"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint . --fix",
    "lint:rules": "npx @eslint/config-inspector",
    "release": "pnpm build && bumpp"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "dependencies": {
    "@antfu/eslint-config": "3.14.0",
    "eslint-plugin-check-file": "^2.8.0",
    "eslint-plugin-tailwindcss": "^3.17.5"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.23.2",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "bumpp": "^9.10.1",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "lint-staged": "^15.4.1",
    "rollup": "^4.30.1",
    "simple-git-hooks": "^2.11.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
